<html>
  <head>
    <%= render partial: "partials/head" %>
  </head>
  <body>

    <h1><%= page.title %></h1>

    <%= render partial: "partials/navigation" %>

    <div class="container-fluid">
      <div class="row-fluid">
        <div class="span12"><%= region :page_header %></div>
      </div>
      <div class="row-fluid">
        <div class="span12"><%= region :above_content %></div>
      </div>
      <div class="row-fluid">
        <div class="span4"><%= region :column_left %></div>
        <div class="span4"><%= region :column_middle %></div>
        <div class="span4"><%= region :column_right %></div>
      </div>
      <div class="row-fluid">
        <div class="span12"><%= region :below_content %></div>
      </div>
      <div class="row-fluid">
        <div class="span12"><%= region :footer %></div>
      </div>
    </div>
    <%= javascript_include_tag "manifest" %>
  </body>
</html>