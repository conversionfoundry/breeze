.form_results-container.container
  %table.fancy.sortable#form_results
    %thead
      %tr
        %th.date.first{:colspan => "1", :style => ""} Date
        %th.form_name Form Name
        %th.values Results
        %th.actions.last{:style => ""} Actions
    %tbody
      - if @form_results.exists?
        = render :partial => "breeze/admin/form_results/form_result", :collection => @form_results
      - else
        %tr
          %td.empty{:colspan => "4"}
            There are no form results yet.
    %tfoot
      %tr